{
    "name": "Shut Your Pi-Hole",
    "description": "Temporarily disable your Pi-hole",
    "version": "0.2",
    "manifest_version": 2,
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "icons": {
        "16": "syph16.png",
        "48": "syph48.png",
       "128": "syph128.png"
    },
    "permissions": [
        "storage",
        "contextMenus"
    ],
    "background": {
        "persistent": false,
        "scripts": [
            "storage.js",
            "syph.js"
        ]
    }
}
